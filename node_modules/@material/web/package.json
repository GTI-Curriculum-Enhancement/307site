    "**/*.js.map",
    "**/*.d.ts",
    "**/*.scss",
    "!css-to-ts.js",
    "!web-test-runner.config.js",
    "!**/test/**",
    "!**/testing/**",
    "!**/*_test.*",
    "!.wireit/**",
